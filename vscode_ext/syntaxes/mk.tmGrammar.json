{
  "scopeName": "example1.dfa",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [{ "include": "#kw" }, { "include": "#constructor" }]
    },
    "kw": {
      "match": "\\b(terminal|transitions|states|start)\\b",
      "name": "keyword.control"
    },
    "constructor": {
      "match": "\\b([A-Z][_0-9A-Za-z]*)\\b",
      "name": "constant.character"
    }
  }
}